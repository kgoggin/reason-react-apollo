{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build All",
      "command": "./node_modules/.bin/lerna",
      "args": [
        "run",
        "build"
      ],
      "problemMatcher": []
    },
    {
      "label": "Generate",
      "type": "npm",
      "script": "generate",
      "path": "packages/tester/",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "build:re:watch",
      "path": "packages/tester/",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "bsb:watch",
      "path": "packages/reason-react-apollo/",
      "group": "build",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "start",
      "path": "website/",
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "build",
      "path": "website/",
      "group": "build",
      "problemMatcher": []
    }
  ]
}
